-- Key System

local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer
local username = localPlayer.Name

-- üö´ Blacklisted usernames
local blacklistedUsers = {
    ["dxrx_123"] = true,
    ["TBizarros"] = true,
    ["Z8_me"] = true,
    ["superjamain"] = true,
    ["MySweetCanDyz"] = true,
}

-- üõë Check if player is blacklisted
if blacklistedUsers[username] then
    localPlayer:Kick(" üò≠  No Script right? SO WHY THE FUCK YOU ARE USING IT? -Cuteeeee  üò≠ ")
    return
end

-- ‚úÖ Allowed Game IDs
local allowedGameIds = {
    [5440820902] = true,
    [6944155317] = true,
}

if not allowedGameIds[game.GameId] then
    game.StarterGui:SetCore("SendNotification", {
        Title = "Cute Hub",
        Text = " üò≠  This script is not available in this game  üò≠",
        Duration = 6
    })
    return
end

-- üü® Bypass System by Username
local instantBypassUsernames = {
    ["XXXGojoXXX100"] = true,
    ["TheRealPanda042"] = true,
    ["mahmoudkhaled234"] = true,
    ["RIPOVERYT"] = true,
    ["HerbertStane"] = true,
    ["hieudepma"] = true,
